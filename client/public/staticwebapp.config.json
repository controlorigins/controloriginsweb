{
  "routes": [
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".tsx": "text/javascript",
    ".ts": "text/javascript",
    ".json": "application/json",
    ".webmanifest": "application/manifest+json",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".svg": "image/svg+xml",
    ".css": "text/css",
    ".html": "text/html"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/**",
      "/*.{js,css,png,jpg,jpeg,gif,svg,ico,webp,woff,woff2,ttf,eot,json,webmanifest}"
    ]
  },
  "globalHeaders": {
    "cache-control": "no-cache"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  }
}
