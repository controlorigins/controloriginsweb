{
  "$schema": "https://json.schemastore.org/staticwebapp.config",
  "routes": [
    {
      "route": "/assets/*",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable",
        "content-encoding": "gzip, br"
      }
    },
    {
      "route": "/*.js",
      "headers": {
        "content-type": "text/javascript; charset=utf-8",
        "cache-control": "public, max-age=31536000, immutable",
        "content-encoding": "gzip, br"
      }
    },
    {
      "route": "/*.mjs",
      "headers": {
        "content-type": "text/javascript; charset=utf-8",
        "cache-control": "public, max-age=31536000, immutable",
        "content-encoding": "gzip, br"
      }
    },
    {
      "route": "/*.css",
      "headers": {
        "content-type": "text/css; charset=utf-8",
        "cache-control": "public, max-age=31536000, immutable",
        "content-encoding": "gzip, br"
      }
    },
    {
      "route": "/*.html",
      "headers": {
        "content-type": "text/html; charset=utf-8",
        "cache-control": "public, max-age=3600, must-revalidate",
        "content-encoding": "gzip, br"
      }
    },
    {
      "route": "/*.png",
      "headers": {
        "content-type": "image/png",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.jpg",
      "headers": {
        "content-type": "image/jpeg",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.jpeg",
      "headers": {
        "content-type": "image/jpeg",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.webp",
      "headers": {
        "content-type": "image/webp",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.avif",
      "headers": {
        "content-type": "image/avif",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.svg",
      "headers": {
        "content-type": "image/svg+xml; charset=utf-8",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.ico",
      "headers": {
        "content-type": "image/x-icon",
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "route": "/*.woff",
      "headers": {
        "content-type": "font/woff",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.woff2",
      "headers": {
        "content-type": "font/woff2",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.ttf",
      "headers": {
        "content-type": "font/ttf",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.eot",
      "headers": {
        "content-type": "application/vnd.ms-fontobject",
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "route": "/*.webmanifest",
      "headers": {
        "content-type": "application/manifest+json; charset=utf-8",
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "route": "/manifest.json",
      "headers": {
        "content-type": "application/manifest+json; charset=utf-8",
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "route": "/site.webmanifest",
      "headers": {
        "content-type": "application/manifest+json; charset=utf-8",
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "route": "/sw.js",
      "headers": {
        "content-type": "text/javascript; charset=utf-8",
        "cache-control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "route": "/robots.txt",
      "headers": {
        "content-type": "text/plain; charset=utf-8",
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "route": "/sitemap.xml",
      "headers": {
        "content-type": "application/xml; charset=utf-8",
        "cache-control": "public, max-age=86400"
      }
    },
    {
      "route": "/*",
      "rewrite": "/index.html"
    }
  ],
  "mimeTypes": {
    ".js": "text/javascript",
    ".mjs": "text/javascript",
    ".tsx": "text/javascript",
    ".ts": "text/javascript",
    ".json": "application/json",
    ".webmanifest": "application/manifest+json",
    ".png": "image/png",
    ".jpg": "image/jpeg",
    ".jpeg": "image/jpeg",
    ".webp": "image/webp",
    ".avif": "image/avif",
    ".svg": "image/svg+xml",
    ".css": "text/css",
    ".html": "text/html",
    ".ico": "image/x-icon",
    ".woff": "font/woff",
    ".woff2": "font/woff2",
    ".ttf": "font/ttf",
    ".eot": "application/vnd.ms-fontobject",
    ".xml": "application/xml",
    ".txt": "text/plain"
  },
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.js",
      "/*.mjs",
      "/*.css",
      "/*.png",
      "/*.jpg",
      "/*.jpeg",
      "/*.gif",
      "/*.svg",
      "/*.ico",
      "/*.webp",
      "/*.avif",
      "/*.woff",
      "/*.woff2",
      "/*.ttf",
      "/*.eot",
      "/*.json",
      "/*.webmanifest",
      "/favicon*",
      "/apple-touch-icon*",
      "/site.webmanifest",
      "/manifest.json",
      "/robots.txt",
      "/sitemap.xml",
      "/sw.js"
    ]
  },
  "globalHeaders": {
    "X-Content-Type-Options": "nosniff",
    "X-Frame-Options": "DENY",
    "X-XSS-Protection": "1; mode=block",
    "Content-Security-Policy": "default-src 'self'; script-src 'self' 'unsafe-inline' https://www.googletagmanager.com https://www.google-analytics.com; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data:; connect-src 'self' https://www.google-analytics.com https://region1.google-analytics.com; object-src 'none'; base-uri 'self'; form-action 'self'; frame-ancestors 'none'; upgrade-insecure-requests",
    "Referrer-Policy": "strict-origin-when-cross-origin",
    "Permissions-Policy": "geolocation=(), microphone=(), camera=(), payment=(), usb=(), magnetometer=(), gyroscope=(), speaker=()",
    "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload",
    "Cache-Control": "public, max-age=0, must-revalidate",
    "X-Robots-Tag": "index, follow",
    "Cross-Origin-Embedder-Policy": "require-corp",
    "Cross-Origin-Opener-Policy": "same-origin",
    "Cross-Origin-Resource-Policy": "cross-origin"
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    },
    "401": {
      "statusCode": 401,
      "rewrite": "/unauthorized.html"
    },
    "403": {
      "statusCode": 403,
      "rewrite": "/forbidden.html"
    }
  },
  "trailingSlash": "auto",
  "platform": {
    "apiRuntime": "node:18"
  }
}
